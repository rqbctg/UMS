
@{
    ViewBag.Title = "UnAllocateClassRoom";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>UnAllocateClassRoom</h2>
<div class="container">
    <div align="center">
        <form method="POST">
            <input id="unassignCourse" type="submit" name="unassignCourse" class="btn btn-danger" onclick="return unassign()" value="UN Allocate ClassRoom" />
        </form>

    </div>
</div>
@if (ViewBag.message != null)
{
    <p>@ViewBag.message</p>
}
@section scripts
{
    <script>

        function unassign() {

            var r = confirm("Are you sure to unassign all courses?");
            if (r === true) {
                return true;
            } else {
                return false;

            }
        }

    </script>
}

